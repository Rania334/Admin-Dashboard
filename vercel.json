
{
  "rewrites": [{ "source": "/(.*)", "destination": "/" }],
  "builds": [
    {
      "src": "angular.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist/dynamic-dashboard"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
      
    }
  ]
}
